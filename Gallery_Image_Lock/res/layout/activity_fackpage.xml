<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/re1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/fack_top_bg" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/btn_back"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/image1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/back"
            android:src="@drawable/whtsap_icon" />

        <RelativeLayout
            android:id="@+id/textlayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/image1" >

            <TextView
                android:id="@+id/chat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:text="Chats"
                android:textColor="@color/white"
                android:textSize="20sp" />

            <EditText
                android:id="@+id/status_text"
                android:layout_width="@dimen/searchtext_width"
                android:layout_height="@dimen/searchtext_height"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:gravity="left"
                android:visibility="invisible"/>

            <requestFocus />
        </RelativeLayout>

        <Button
            android:id="@+id/btnserch"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginRight="55dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_toRightOf="@id/textlayout"
            android:background="@drawable/fack_search_unpresed"
            android:gravity="center" />

        <Button
            android:id="@+id/btnfack"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_margin="15dp"
            android:background="@drawable/contactxml"
            android:gravity="center"
            android:visibility="visible" />
    </RelativeLayout>

    <LinearLayout
